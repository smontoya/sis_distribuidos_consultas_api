<h1>nuevo usuario</h1>

<%= form_for :usuario, :url => { :action => "create" } do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>
 
  <p>
    <%= f.label :nombres %><br>
    <%= f.text_field :nombres %>
  </p>
 
   <p>
    <%= f.label :apellidos %><br>
    <%= f.text_field :apellidos %>
  </p>

  <p>
    <%= f.submit :'Agregar usuario' %>
  </p>
<% end %>